<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Pane prefHeight="54.0" prefWidth="600.0" style="-fx-background-color: #36486b#36486b;" />
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#reportbutton" style="-fx-background-color: #36486b#36486b;" text="Retrun to form page " textFill="#fefbd8" />
      <TableView fx:id="reporttbl" layoutX="34.0" layoutY="77.0" prefHeight="279.0" prefWidth="532.0">
        <columns>
          <TableColumn fx:id="Incidentcol" prefWidth="75.0" text="Incident" />
          <TableColumn fx:id="Locationcol" prefWidth="132.0" text="Location" />
            <TableColumn fx:id="Addinfocol" prefWidth="133.0" text="Additional Information" />
            <TableColumn fx:id="Contactcol" prefWidth="75.0" text="Contact" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
